<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ProductDetailDao">
	<select id="getProductDetails" resultType="com.pojo.ProductDetail" parameterType="int">
		select * from ego_product where fid = #{fid}
	</select>	
	
	<select id="getProductFamilies" resultType="com.pojo.ProductFamily" parameterType="int">
	 	select * from ego_family where fid = #{fid}
	</select>
</mapper>